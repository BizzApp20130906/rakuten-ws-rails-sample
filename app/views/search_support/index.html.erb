<h1>SearchSupport#index</h1>

<p>ボタンであとでリンクする</p>
<p>HelloWorld</p>

<form action="<%= search_support_path %>" method="POST">
    <input type="input" name="keyword" required>
    <input type="submit" value="Search">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
</form>

<% if ! @jsonData.nil? %>
<ul>
    <% @jsonData['Items'].each do |item| %>
    <li><%= link_to item['Item']['itemName'], item['Item']['affiliateUrl'] %></li>
    <% end %>
</ul>
<% end %>
